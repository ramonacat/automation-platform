{
    "build": {
        "docker": {
            "build-command": [
                "docker",
                "buildx",
                "build",
                "--cache-from",
                "type=local,src=/mnt/.buildx-cache",
                "--cache-to",
                "type=local,dest=/mnt/.buildx-cache-new,mode=max"
            ]
        }
    }
}
