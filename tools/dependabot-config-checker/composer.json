{
    "name": "ramona/automation-platform-tool-dependabot-config-checker",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Ramona\\AutomationPlatformToolDependabotConfigChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Ramona\\AutomationPlatformToolDependabotConfigChecker\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../libraries/php/coding-standard/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "symfony/yaml": "^5.3",
        "thecodingmachine/safe": "^1.3"
    },
    "require-dev": {
        "ramona/automation-platform-lib-coding-standard": "*",
        "symplify/easy-coding-standard": "^11.0",
        "vimeo/psalm": "^5.1",
        "phpunit/phpunit": "^9.5",
        "infection/infection": "^0.26.0",
        "slevomat/coding-standard": "^8.0"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
